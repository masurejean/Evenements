{% extends 'base.html.twig' %}

{% block title %}Hello UtilisateurController!{% endblock %}

{% block body %}
{{form_start(formulaire)}}
<div class="form-control">
    <div class="form-group">
        {{form_row(formulaire.email)}}
    </div>
    <div class="form-group">
        {{form_row(formulaire.password)}}
    </div>
    <div class="form-group">
        {{form_row(formulaire.genre)}}
    </div>
    
</div>
<button type = "submit" class="btn btn-success">Ajouter un employe</button>


{% endblock body %}
